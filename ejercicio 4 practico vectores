#include <iostream>
#include <vector>
using namespace std;

int main(){
    vector<int> a = {1,2,3,4,5};
    vector<int> b(a.size());

    for(int i=0; i<a.size(); i++)
        b[i] = a[a.size()-1-i];

    cout << "Vector original: ";
    for(int x : a) cout << x << " ";

    cout << "\nVector invertido: ";
    for(int x : b) cout << x << " ";
}
