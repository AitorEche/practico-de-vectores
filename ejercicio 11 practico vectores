#include <iostream>
#include <vector>
using namespace std;

int main(){
    vector<int> v = {-3, 0, 8, -2, 5, 0};
    int neg=0, pos=0, cer=0;

    for(int x : v){
        if(x < 0) neg++;
        else if(x > 0) pos++;
        else cer++;
    }

    cout << "Negativos: " << neg << endl;
    cout << "Positivos: " << pos << endl;
    cout << "Ceros: " << cer << endl;
}
